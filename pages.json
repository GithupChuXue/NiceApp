		{
			"path": "pages/index/index",
			"style": {}
		}, {
			"path": "pages/news/news",
			"style": {
				"enablePullDownRefresh": true
			}

		}, {
			"path": "pages/more/more",
			"style": {
				"enablePullDownRefresh": true
			}

		}, {
			"path": "pages/userInfo/userInfo",
